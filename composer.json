{
    "name": "sneakyx/laravel-dynamic-encryption",
    "version": "0.5.0",
    "description": "Laravel package that replaces the default encrypter with a dynamic key loaded from cache or database.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sneakyx\\LaravelDynamicEncryption\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/encryption": "^10.0|^11.0|^12.0",
        "illuminate/cache": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Sneakyx\\LaravelDynamicEncryption\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sneakyx\\LaravelDynamicEncryption\\Providers\\DynamicEncryptionServiceProvider"
            ]
        }
    },
    "keywords": [
        "laravel",
        "encryption",
        "security",
        "kdf",
        "pbkdf2",
        "argon2id"
    ],
    "homepage": "https://github.com/sneakyx/laravel-dynamic-encryption",
    "authors": [
        {
            "name": "Andr√© Scholz",
            "email": "info@rothaarsystems.de"
        }
    ],
    "support": {
        "issues": "https://github.com/sneakyx/laravel-dynamic-encryption/issues",
        "source": "https://github.com/sneakyx/laravel-dynamic-encryption",
        "docs": "https://github.com/sneakyx/laravel-dynamic-encryption/tree/main/docs"
    }
}
